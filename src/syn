#/bin/bash
# If you change the code, the changes must syn to every node.
# This shell script is used to synchonize the changes

rsync -avz ~/zhangjian/code/ node0:~/zhangjian/code/
rsync -avz ~/zhangjian/code/ node2:~/zhangjian/code/
rsync -avz ~/zhangjian/code/ node3:~/zhangjian/code/
rsync -avz ~/zhangjian/code/ node4:~/zhangjian/code/
rsync -avz ~/zhangjian/code/ node5:~/zhangjian/code/
rsync -avz ~/zhangjian/code/ node6:~/zhangjian/code/

